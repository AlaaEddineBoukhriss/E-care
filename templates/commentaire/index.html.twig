<div class="container contact-form">

    <form method="post">
        {% block body2 %}

        <BODY>
        <P><FONT color="#00a4e4"><b>Questions médicales</b></FONT>

            <br />
            <br />
            <br />


            <input type="text" id="search" name="search" class="form-control" placeholder="Search">

            {% for message in app.flashes('info')  %}
            <div class="alert-success">
                {{ message }}
            </div>
            {% endfor %}
            <br />
            {#<input type="text" id="search" class="form-control"
                   placeholder="Search"> #}
            <script src="{{ asset('js/jquery.min.js') }}"></script>
            <table id="tableac" class="table table-striped table-advance table-hover">

                <thead>
                <tr>
                    <th>Patient </th>
                    <th>Sujet</th>
                    <th>Question</th>
                    <th>Médecin </th>

                </tr>
                </thead>
                <tbody>

                {% for comme in commentaires %}
                    <tr>

                        <td>{{ comme.pseudo }}</td>
                        <td>{{ comme.sujet }}</td>
                        <td>{{ comme.question }}</td>
                        <td>{{ comme.medecin }}</td>



                        <td>
                            <a href="{{ path('commentaire_show' , {'id': comme.id} ) }}">Show</a>
                            <br />
                            <br />

                            <a href="{{ path('showreponse', {'id': comme.id}) }}">Voir réponse</a>
                            <br />
                            <br />

                            <a href="{{ path('reponse', {'id': comme.id}) }}">Reply</a>
                            <br />
                            <br />

                            <br />
                            <br />
                        </td>

                    </tr>


                {% else %}
                    <tr>
                        <td colspan="6">Aucun sujet</td>
                    </tr>
                {% endfor %}


                <script>
                    $(document).ready(function(){
                        $('#search').keyup(function(){
                            search_table($(this).val());
                        });
                        function search_table(value){
                            $('#tableac tbody tr').each(function(){
                                var found = 'false';
                                $(this).each(function(){
                                    if($(this).text().toLowerCase().indexOf(value.toLowerCase()) >= 0)
                                    {
                                        found = 'true';
                                    }
                                });
                                if(found == 'true')
                                {
                                    $(this).show();

                                }
                                else
                                {
                                    $(this).hide();

                                }
                            });
                        }
                    });
                </script>
                </tbody>

            </table>

            <a href="{{ path('commentaire_new') }}">Posez votre question!</a>
            <br />
            <br />

            {{ knp_pagination_render(commentaires) }}





        {% endblock %}
    </form>
